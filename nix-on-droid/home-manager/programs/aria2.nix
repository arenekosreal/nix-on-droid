{
    enable = true;
    settings = {
      dir = "\${HOME}/storage/Downloads";
      check-integrity = true;
      continue = true;
      no-netrc = true;
      remote-time = true;
      http-accept-gzip = true;
      bt-enable-lpd = true;
      bt-save-metadata = true;
      enable-dht6 = true;
      daemon = false;
      enable-mmap = true;
      file-allocation = "falloc";
      optimize-concurrent-downloads = true;
    };
}
